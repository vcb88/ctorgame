{
  "name": "@ctor-game/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types/base": {
      "types": "./src/types/base/index.ts",
      "import": "./src/types/base/index.ts",
      "default": "./src/types/base/index.ts"
    },
    "./types/base/*": {
      "types": "./src/types/base/*.ts",
      "import": "./src/types/base/*.ts",
      "default": "./src/types/base/*.ts"
    },
    "./types/game": {
      "types": "./src/types/game/index.ts",
      "import": "./src/types/game/index.ts",
      "default": "./src/types/game/index.ts"
    },
    "./types/game/*": {
      "types": "./src/types/game/*.ts",
      "import": "./src/types/game/*.ts",
      "default": "./src/types/game/*.ts"
    },
    "./types/network": {
      "types": "./src/types/network/index.ts",
      "import": "./src/types/network/index.ts",
      "default": "./src/types/network/index.ts"
    },
    "./types/storage": {
      "types": "./src/types/storage/index.ts",
      "import": "./src/types/storage/index.ts",
      "default": "./src/types/storage/index.ts"
    },
    "./types/replay": {
      "types": "./src/types/replay/index.ts",
      "import": "./src/types/replay/index.ts",
      "default": "./src/types/replay/index.ts"
    },
    "./types/replay/*": {
      "types": "./src/types/replay/*.ts",
      "import": "./src/types/replay/*.ts",
      "default": "./src/types/replay/*.ts"
    },
    "./types/redis": {
      "types": "./src/types/redis/index.ts",
      "import": "./src/types/redis/index.ts",
      "default": "./src/types/redis/index.ts"
    },
    "./validation/*": {
      "types": "./src/validation/*.ts",
      "import": "./src/validation/*.ts",
      "default": "./src/validation/*.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "import": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.6.3",
    "eslint": "^8.55.0"
  }
}