{
  "name": "@ctor-game/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "default": "./dist/types/*.js"
    },
    "./types/game": {
      "types": "./dist/types/game/index.d.ts",
      "import": "./dist/types/game/index.js",
      "default": "./dist/types/game/index.js"
    },
    "./types/game/*": {
      "types": "./dist/types/game/*.d.ts",
      "import": "./dist/types/game/*.js",
      "default": "./dist/types/game/*.js"
    },
    "./types/network": {
      "types": "./dist/types/network/index.d.ts",
      "import": "./dist/types/network/index.js",
      "default": "./dist/types/network/index.js"
    },
    "./types/storage": {
      "types": "./dist/types/storage/index.d.ts",
      "import": "./dist/types/storage/index.js",
      "default": "./dist/types/storage/index.js"
    },
    "./types/replay": {
      "types": "./dist/types/replay/index.d.ts",
      "import": "./dist/types/replay/index.js",
      "default": "./dist/types/replay/index.js"
    },
    "./types/replay/*": {
      "types": "./dist/types/replay/*.d.ts",
      "import": "./dist/types/replay/*.js",
      "default": "./dist/types/replay/*.js"
    },
    "./types/redis": {
      "types": "./dist/types/redis/index.d.ts",
      "import": "./dist/types/redis/index.js",
      "default": "./dist/types/redis/index.js"
    },
    "./validation/*": {
      "types": "./dist/validation/*.d.ts",
      "import": "./dist/validation/*.js",
      "default": "./dist/validation/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "default": "./dist/utils/*.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo && rm -f ../server/tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.6.3",
    "eslint": "^8.55.0"
  }
}