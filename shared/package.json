{
  "name": "@ctor-game/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsc && tsc -p tsconfig.cjs.json && for f in dist/cjs/*.js; do mv \"$f\" \"${f%.js}.cjs\"; done",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}