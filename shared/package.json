{
  "name": "@ctor-game/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./types": {
      "types": "./dist/types/base/types.d.ts",
      "import": "./dist/types/base/types.js",
      "default": "./dist/types/base/types.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "default": "./dist/utils/*.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo && rm -f ../server/tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "5.3.3",
    "eslint": "^8.55.0"
  }
}